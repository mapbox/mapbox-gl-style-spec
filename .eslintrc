{
  "parser": "espree",
  "rules": {
    "space-after-function-name": 2,
    "space-in-parens": 2,
    "space-before-blocks": 2,
    "space-after-keywords": 2,
    "space-unary-ops": 2,
    "no-use-before-define": [2, "nofunc"],
    "camelcase": 0,
    "comma-style": 2,
    "no-lonely-if": 2,
    "no-else-return": 0,
    "eqeqeq": 0,
    "new-cap": 2,
    "no-empty": 2,
    "no-new": 2,
    "key-spacing": 2,
    "no-multi-spaces": 2,
    "brace-style": 2,
    "no-throw-literal": 2,
    "no-self-compare": 2,
    "no-void": 2,
    "wrap-iife": 2,
    "no-eq-null": 2,
    "quotes": [0],
    "curly": 0,
    "space-infix-ops": [0],
    "space-unary-ops": [0],
    "dot-notation": [0],
    "no-lonely-if": [0],
    "space-return-throw-case": [0],
    "no-shadow": 0
  },
  "env": {
    "node": true,
    "browser": true
  }
}
