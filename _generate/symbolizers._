<% _(symbolizers).each(function(symbolizer, name) { %>
  <% if (name !== '$version' && name !== '$root') { %><h2 id='<%= name %>'><%= name.replace('_', ': ') %></h2><% } %>
  <div class='space-bottom4 clearfix'>
  <% _(symbolizer).each(function(config, property) { %>
    <% if (_.isObject(config)) { %>
      <% if (config.fn) { %>
        <div class='space-bottom2 clearfix'><p><em class='pad2y'><%= config.fn.values %> function:</em></p>
        <% _(config).each(function(fn, fnProperty) { %>
        <div class='space-bottom2 property clearfix'>
          <div class='space-bottom1'>
            <strong class='space-right'><%= fnProperty %></strong>
            <% if (fn.type && fn.type !== '*') { %><span class='small space-right'><a href='#<%= fn.type %>'><%= fn.type %></a></span><% } %>
            <% if (fn.values) { %><span class='small quiet space-right'><em>Options:</em> <% _(fn.values).each(function(v) { %><strong><%= v %></strong> <% }); %></span><% } %>
            <% if (fn.default) { %><span class='small quiet space-right'><em>Default:</em> <%= fn.default %> </span><% } %>
            <% if (fn.required) { %><span class='icon alert quiet inline' title='Required'>&nbsp;</span> <% } %>
            <% if (fn.function) { %><span class='icon levels quiet inline' title='Supports function values'>&nbsp;</span><% } %>
            <% if (fn.transition) { %><span class='icon opacity quiet inline' title='Transitionable'>&nbsp;</span><% } %>
          </div>
          <% if (fn.doc) { %><p><%= fn.doc %></p><% } %>
        </div>
        <% }); %>
        </div>
      <% } else { %>
        <div class='space-bottom2 property'>
          <div class='space-bottom1'>
            <% if (_.isString(property)) { %><strong class='space-right'><%= property %></strong><% } %>
            <% if (config.type && config.type !== '*') { %><span class='small space-right'><a href='#<%= config.type %>'><%= config.type.replace('_',': ') %></a></span><% } %>
            <% if (config.values) { %><span class='small quiet space-right'><em>Options:</em> <% _(config.values).each(function(v) { %><strong><%= v %></strong> <% }); %></span><% } %>
            <% if (config.default) { %><span class='small quiet space-right'><em>Default:</em> <%= config.default %> </span><% } %>
            <% if (config.required) { %><span class='icon alert quiet inline' title='Required'>&nbsp;</span> <% } %>
            <% if (config.function) { %><span class='icon levels quiet inline' title='Supports function values'>&nbsp;</span><% } %>
            <% if (config.transition) { %><span class='icon opacity quiet inline' title='Transitionable'>&nbsp;</span><% } %>
          </div>
          <% if (config.doc) { %><p><%= config.doc %></p><% } %>
        </div>
      <% } %>
    <% } else if (property !== 'doc') { %>
      <% if (config) { %><span class='small strong block'><a href='#<%= config %>'><%= config.replace('_',': ') %></a></span><% } %>
    <% } %>
  <% }); %>
  <% if (symbolizer.doc) { %><p><%= symbolizer.doc %></p><% } %>
  </div>
<% }); %>
